{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnections": "${DB_CONNECTION_STRING_STAGING}",
    "IdentityConnections": "${DB_IDENTITY_CONNECTION_STRING_STAGING}"
  },
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY_STAGING}",
    "Issuer": "MovieReservation",
    "Audience": "MovieReservationUsers",
    "ExpirationMinutes": 60
  },
  "Stripe": {
    "SecretKey": "${STRIPE_SECRET_KEY_STAGING}",
    "PublishableKey": "${STRIPE_PUBLISHABLE_KEY_STAGING}"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://staging.moviereservation.com"
    ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH" ],
    "AllowedHeaders": [ "Authorization", "Content-Type", "Accept" ]
  },
  "ResponseCompression": {
    "EnableForHttps": true,
    "CompressionLevel": "Optimal"
  }
}