{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnections": "Server=.;Database=MovieReservation_Dev;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True;",
    "IdentityConnections": "Server=.;Database=MovieReservation_Identity_Dev;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True;"
  },
  "Jwt": {
    "SecretKey": "YourVeryLongSecretKeyThatIsAtLeast32CharactersLongForHmacSha256Development!",
    "Issuer": "MovieReservation",
    "Audience": "MovieReservationUsers",
    "ExpirationMinutes": 60
  },
  "Stripe": {
    "SecretKey": "sk_test_your_stripe_secret_key_here",
    "PublishableKey": "pk_test_your_stripe_publishable_key_here"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "http://127.0.0.1:3000",
      "http://127.0.0.1:5173"
    ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS" ],
    "AllowedHeaders": [ "*" ]
  },
  "ResponseCompression": {
    "EnableForHttps": true,
    "CompressionLevel": "Fastest"
  },
  "Redis": {
    "Enabled": true,
    "Connection": "localhost:6379,abortConnect=false,connectTimeout=30000,defaultDatabase=0"
  }
}